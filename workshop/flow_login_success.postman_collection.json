							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Your test name\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(10).to.eql(jsonData.length);",
							"",
							"    // Set variable user_id",
							"    pm.collectionVariables.set(\"user_id\", jsonData[0].id);",
